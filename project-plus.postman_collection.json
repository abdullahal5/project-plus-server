		{
			"name": "Auth",
			"item": [
				{
					"name": "Login",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const response = pm.response.json();\r",
									"\r",
									"if (response.success && response.data && response.data.accessToken) {\r",
									"    if (response.data.role === \"ADMIN\") {\r",
									"        pm.collectionVariables.set(\"ADMIN_TOKEN\", `Bearer ${response.data.accessToken}`);\r",
									"    } else if (response.data.role === \"MANAGER\") {\r",
									"        pm.collectionVariables.set(\"MANAGER_TOKEN\", `Bearer ${response.data.accessToken}`);\r",
									"    } else {\r",
									"        pm.collectionVariables.set(\"MEMBER_TOKEN\", `Bearer ${response.data.accessToken}`);\r",
									"    }\r",
									"} else {\r",
									"    console.warn(\"Login failed or role mismatch. Token not set.\");\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "{{",
								"type": "text"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"email\": \"member@gmail.com\",\r\n    \"password\": \"123456\"\r\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{BASE_URL}}/auth/login",
							"host": [
								"{{BASE_URL}}"
							],
							"path": [
								"auth",
								"login"
							]
						}
					},
					"response": []
				},
				{
					"name": "Refresh token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const response = pm.response.json();\r",
									"\r",
									"if (response.success && response.data && response.data.accessToken) {\r",
									"    const token = `Bearer ${response.data.accessToken}`;\r",
									"    const role = response.data.role;\r",
									"\r",
									"    switch (role) {\r",
									"        case \"ADMIN\":\r",
									"            pm.collectionVariables.set(\"ADMIN_TOKEN\", token);\r",
									"            console.log(\"ADMIN token set\");\r",
									"            break;\r",
									"        case \"MANAGER\":\r",
									"            pm.collectionVariables.set(\"MANAGER_TOKEN\", token);\r",
									"            console.log(\"MANAGER token set\");\r",
									"            break;\r",
									"        case \"MEMBER\":\r",
									"            pm.collectionVariables.set(\"MEMBER_TOKEN\", token);\r",
									"            console.log(\"MEMBER token set\");\r",
									"            break;\r",
									"        default:\r",
									"            console.warn(\"Unknown role. Token not set.\");\r",
									"    }\r",
									"} else {\r",
									"    console.warn(\"Login/Refresh failed. Token not set.\");\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [],
						"url": {
							"raw": "{{BASE_URL}}/auth/refresh-token",
							"host": [
								"{{BASE_URL}}"
							],
							"path": [
								"auth",
								"refresh-token"
							]
						}
					},
					"response": []
				}
			]
